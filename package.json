{
  "name": "todolist-app",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "scripts": {
    "dev": "concurrently \"pnpm dev:next\" \"pnpm dev:server\"",
    "dev:next": "next dev",
    "dev:server": "tsx src/app/server/index.ts",
    "build": "next build && pnpm build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "next start",
    "start:server": "node dist/app/server/index.js",
    "lint": "eslint --fix",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
