    "@heroui/alert": "^2.2.21",
    "@heroui/badge": "^2.2.12",
    "@heroui/button": "2.2.19",
    "@heroui/card": "^2.2.18",
    "@heroui/checkbox": "^2.3.18",
    "@heroui/chip": "^2.2.15",
    "@heroui/code": "2.2.14",
    "@heroui/divider": "^2.2.13",
    "@heroui/dropdown": "^2.3.19",
    "@heroui/input": "2.4.19",
    "@heroui/kbd": "2.2.15",
    "@heroui/link": "2.2.16",
    "@heroui/listbox": "2.3.18",
    "@heroui/navbar": "2.2.17",
    "@heroui/select": "^2.4.19",
    "@heroui/snippet": "2.2.20",
    "@heroui/switch": "2.2.17",
    "@heroui/system": "2.4.15",
    "@heroui/theme": "2.4.15",
    "@heroui/toast": "^2.0.11",
    "@react-aria/ssr": "3.9.8",
    "@react-aria/visually-hidden": "3.8.22",
    "@sentry/nextjs": "^9.27.0",
    "@sentry/node": "^7.92.0",
    "clsx": "2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "framer-motion": "11.13.1",
    "intl-messageformat": "10.7.16",
    "mysql2": "^3.14.1",
    "next": "14.2.29",
    "next-themes": "0.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0"
